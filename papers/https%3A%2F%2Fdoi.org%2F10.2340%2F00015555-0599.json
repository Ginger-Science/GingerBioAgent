  },
  "fabio:hasPart": [
    {
      "@id": "sec1",
      "@type": "fabio:Section",
      "dcterms:title": "Introduction",
      "fabio:hasContent": "The introduction outlines the prevalence of PLE and the challenges associated with its prevention and treatment."
    },
    {
      "@id": "sec2",
      "@type": "fabio:Section",
      "dcterms:title": "Materials and Methods",
      "fabio:hasContent": "Twelve patients with a history of PLE participated in this study using a sunscreen with very high protection level."
    },
    {
      "@id": "sec3",
      "@type": "fabio:Section",
      "dcterms:title": "Results",
      "fabio:hasContent": "PLE was prevented in all patients using the sunscreen under standardized conditions."
    },
    {
      "@id": "sec4",
      "@type": "fabio:Section",
      "dcterms:title": "Discussion",
      "fabio:hasContent": "The study demonstrated the efficacy of the sunscreen in preventing PLE, with results supporting further examination of regular sunscreen application."
    }
